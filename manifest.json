{
  "manifest_version": 2,

  "name": "Colorful Code Blocks",
  "description": "Syntax highlighting for dull, hard to read, code boxes across the web.",
  "version": "0.1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click Here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
